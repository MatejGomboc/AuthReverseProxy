# http://editorconfig.org
# VSCode: https://marketplace.visualstudio.com/items?itemName=EditorConfig.EditorConfig

root = true

[*]
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.sh]
end_of_line = lf

[*.sln]
indent_style = tab
